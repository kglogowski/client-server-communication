<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!---->
        <!--Request-->
        <service id="csc.factory.pager_request_model" class="CSC\Server\Request\Factory\PagerRequestModelFactory">
        </service>
        <service id="csc.factory.plain_pager_request_model" class="CSC\Server\Request\Factory\PlainPagerRequestModelFactory">
        </service>

        <!--Response-->
        <service id="csc.factory.response_model" class="CSC\Server\Response\Factory\DefaultResponseModelFactory">
        </service>
    </services>
</container>
