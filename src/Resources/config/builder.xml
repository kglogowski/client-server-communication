<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!--General-->
        <service id="csc.builder.filter" class="CSC\Builder\QueryFilterBuilder">
        </service>

        <!--Rest-->
        <service id="csc.rest.builder.pager_query" class="CSC\Protocol\Rest\Builder\RestPagerQueryBuilder">
            <argument type="service" id="csc.builder.filter" />
            <argument type="service" id="csc.checker.query_parameter" />
        </service>
    </services>
</container>
