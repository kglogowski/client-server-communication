<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <!--General-->
        <service id="csc.executor.insert" class="CSC\Component\Executor\InsertExecutor">
            <argument type="service" id="csc.entity_manager.provider" />
        </service>
        <service id="csc.executor.merge" class="CSC\Component\Executor\MergeExecutor">
            <argument type="service" id="csc.entity_manager.provider" />
        </service>
        <service id="csc.executor.delete" class="CSC\Component\Executor\DeleteExecutor">
            <argument type="service" id="csc.entity_manager.provider" />
        </service>
        <!---->
        <service id="csc.executor.patch" class="CSC\Component\Executor\PatchExecutor">
            <argument type="service" id="csc.entity_manager.provider" />
            <argument type="service" id="csc.provider.get_element" />
        </service>
    </services>
</container>
