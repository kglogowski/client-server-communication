<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="csc.rest.data_object_manager.pager" class="CSC\Protocol\Rest\Server\Manager\RestDataObjectManager">
            <argument type="service" id="csc.rest.request_processor.pager" />
            <argument type="service" id="csc.rest.response_processor" />
        </service>
        <service id="csc.rest.data_object_manager.plain_pager" class="CSC\Protocol\Rest\Server\Manager\RestDataObjectManager">
            <argument type="service" id="csc.rest.request_processor.plain_pager" />
            <argument type="service" id="csc.rest.response_processor" />
        </service>
        <!--<service id="csc.rest.data_object_manager.simple" class="CSC\Protocol\Rest\Server\Manager\RestDataObjectManager">-->
        <!--</service>-->
    </services>
</container>
